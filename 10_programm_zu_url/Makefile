bin: main.cpp
	clang++ main.cpp -o bin --sysroot=/usr/cxxlib/release -std=c++20 -O3 -std=c++20 -nostdinc -nostdinc++ -isystem /usr/cxxlib/release/include/c++/v1 -isystem /usr/cxxlib/release/include/clang -isystem /usr/cxxlib/release/include/x86_64-linux-gnu -isystem /usr/cxxlib/release/include -isystem /usr/include -Wno-unused-command-line-argument -nostdlib /usr/cxxlib/release/lib/libunwind.a /usr/cxxlib/release/lib/libc++.a /usr/cxxlib/release/lib/crt1.o /usr/cxxlib/release/lib/crti.o /usr/cxxlib/release/lib/crtn.o /usr/cxxlib/release/lib/libdl.so.2 /usr/cxxlib/release/lib/librt.a /usr/cxxlib/release/lib/libm.a /usr/cxxlib/release/lib/crtn.o /usr/lib/clang/16/lib/linux/libclang_rt.builtins-x86_64.a /usr/lib/clang/16/lib/linux/clang_rt.crtbegin-x86_64.o /usr/lib/clang/16/lib/linux/clang_rt.crtend-x86_64.o /usr/cxxlib/release/lib/libc.so -Wl,-s
